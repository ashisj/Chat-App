<div class="row">
    <div class="col-sm-9">
        <div class="header"></div>
        <div id="messages"></div>
        <form class="row" style="margin-top: 2px" id="msgForm">
            <div class="col-sm-11">
                <input id="name" type="hidden" value="{{user.name}}">
                <input id="username" type="hidden" value="{{user.username}}">

                <input id="message" autocomplete="off" autofocus="on" placeholder="type your message here..." class="form-control"/>
            </div>
            <div class="col-sm-1">
                <button id="send" class="btn btn-success" style="padding: auto 4px; float:right">Send</button>
            </div>
        </form>
    </div>
    <div class="col-sm-3">
        <div class="chat-users">
          <div id = "sipu">
            <button data-toggle="collapse" data-target="#onlineUSers" id="onlineUsersButton" class="btn btn-primary btn-block online-user-button-position">Online Users</button>
            <div id="onlineUSers" class="collapse"></div>
          </div>
        </div>
    </div>
</div>

<script src="/socket.io/socket.io.js"></script>

<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet">

